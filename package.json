{
  "name": "cat-shop",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "express_start": "node server.js",
    "dev:gen": "concurrently -r \"next dev\" \"npx graphql-codegen --watch\"",
    "dev:test": "concurrently -r \"next dev\" \"ts-node-dev --no-notify --respawn --transpile-only nexus/server\"",
    "nexus-typegen": "set BUILD_ONLY=true && node -r esbuild-register ./nexus/resolvers.ts",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "prepare": "husky install",
    "generate": "npx prisma generate",
    "generate:nexus": "ts-node --transpile-only -P nexus.tsconfig.json ./nexus/resolvers.ts",
    "model-push": "npx prisma db push",
    "seed": "npx prisma db seed"
  },
  "type": "commonjs",
  "prisma": {
    "seed": "ts-node --transpile-only prisma/seed.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "@apollo/server": "4.9.5",
    "@as-integrations/next": "^2.0.2",
    "@chakra-ui/icons": "^2.0.13",
    "@chakra-ui/react": "^2.4.3",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@prisma/client": "5.5.2",
    "express": "^4.18.2",
    "framer-motion": "^6",
    "graphql": "16.8.1",
    "graphql-scalars": "1.22.4",
    "graphql-tag": "^2.12.6",
    "isomorphic-unfetch": "^4.0.2",
    "next": "13.5.6",
    "next-urql": "^5.0.2",
    "nexus": "1.3.0",
    "nexus-prisma": "2.0.1",
    "prettier": "^3.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.48.2",
    "react-is": "^18.2.0",
    "urql": "^2.2.3",
    "urql-serialize-scalars-exchange": "^1.0.8",
    "winston": "^3.8.2",
    "xstate": "^4.35.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@graphql-codegen/typescript-urql": "^4.0.0",
    "@parcel/watcher": "^2.3.0",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18",
    "@xstate/react": "^3.0.1",
    "autoprefixer": "^10.0.1",
    "concurrently": "^8.2.2",
    "eslint": "^8",
    "eslint-config-next": "14.0.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "postcss": "^8",
    "prisma": "5.5.2",
    "tailwindcss": "^3.2.4",
    "ts-node": "10.9.1",
    "ts-node-dev": "2.0.0",
    "typescript": "5.0.4"
  }
}
